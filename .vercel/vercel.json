{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "10mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/:1",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "env": {
      "FLASK_ENV": "production"
    }
  }
  